{
  "name": "gardr-host",
  "version": "0.4.0",
  "description": "The js part of Gardr which insert and talks with the iframes on the host page",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/jshint --reporter node_modules/jshint-stylish/stylish.js lib test --exclude test/lib",
    "test": "npm run lint && ./node_modules/karma/bin/karma start --single-run",
    "watch": "npm run lint && ./node_modules/karma/bin/karma start",
    "dist": "mkdir -p dist && ./node_modules/.bin/browserify lib/index.js -d -s gardr.host -o dist/gardr.host.js && ./node_modules/.bin/uglifyjs dist/gardr.host.js -mc -o dist/gardr.host.min.js"
  },
  "homepage": "https://github.com/gardr/host",
  "repository": {
    "type": "git",
    "url": "git@github.com:gardr/host.git"
  },
  "keywords": [
    "gardr",
    "iframe",
    "ads",
    "third-party"
  ],
  "author": "FINN.no AS",
  "contributors": [
    "Gregers Rygg <gregers@finn.no>",
    "Michael Gunnulfsen <michael.gunnulfsen@finn.no>",
    "Sveinung Rosaker <sveinung.rosaker@finn.no>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gardr/host/issues"
  },
  "devDependencies": {
    "karma-chai": "~0.1.0",
    "jshint-stylish": "~0.1.5",
    "chai": "~1.9.0",
    "karma-browserify": "~0.1.0",
    "mocha": "~1.17.1",
    "jshint": "~2.4.3",
    "karma-sinon-chai": "~0.1.4",
    "browserify": "~3.30.1",
    "karma-script-launcher": "~0.1.0",
    "karma-chrome-launcher": "~0.1.2",
    "karma-firefox-launcher": "~0.1.3",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.1.5",
    "requirejs": "~2.1.11",
    "karma-requirejs": "~0.2.1",
    "karma-coffee-preprocessor": "~0.1.3",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma": "~0.10.9",
    "karma-mocha": "~0.1.1",
    "uglifyjs": "~2.3.6"
  },
  "dependencies": {
    "util-extend": "~1.0.1",
    "query-params": "0.0.1",
    "eventlistener": "0.0.1",
    "cross-domain-events": "0.0.2",
    "gardr-core-plugin": "0.0.1"
  }
}
